{
  "name": "codius-web",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "format": "npm run prettier && npm run lint --fix --quiet",
    "lint": "eslint . --ext .tsx",
    "lint:fix": "npm run lint -- --fix",
    "precommit": "lint-staged",
    "prettier": "prettier --write './**/*.{tsx,md}'",
    "start": "next start"
  },
  "lint-staged": {
    "*.{md}": [
      "prettier --write"
    ],
    "*.tsx": [
      "prettier --write",
      "npm run lint --fix"
    ]
  },
  "dependencies": {
    "@prantlf/jsonlint": "^10.2.0",
    "ace-builds": "^1.4.12",
    "js-yaml": "^3.14.0",
    "next": "9.4.1",
    "react": "16.12.0",
    "react-ace": "^9.1.1",
    "react-dom": "16.12.0",
    "react-string-replace": "^0.4.4",
    "react-web-monetization": "0.0.13",
    "recoil": "0.0.10"
  },
  "devDependencies": {
    "@types/node": "^13.5.0",
    "@types/react": "^16.9.19",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-standard": "^4.0.1",
    "prettier": "^2.0.5",
    "typescript": "^3.9.7"
  }
}
